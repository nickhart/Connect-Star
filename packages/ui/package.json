{
  "name": "@connect-star/ui",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/components && cp src/components/*.css dist/components/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@connect-star/game-logic": "workspace:*",
    "@connect-star/types": "workspace:*",
    "@simple-game-server/client": "file:../../../simple-game-server/clients/typescript/simple-game-server-client-0.1.0.tgz",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "18.2.0"
  }
}
