{
  "name": "connect-star",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "test:coverage": "jest --config jest.config.workspace.js --coverage",
    "test:ci": "jest --config jest.config.workspace.js --coverage --ci --watchAll=false",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "turbo": "^2.5.4"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.12.1"
}
